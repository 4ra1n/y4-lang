函数 是否在数组中(数组, 元素) {
    循环 甲=0; 甲<长度(数组); 甲=甲+1 {
        临时 = 数组[甲];
        如果 临时 == 元素 {
            返回 真的;
        }
    }
    返回 假的;
}

函数 去重(数组) {
    结果 = [];
    循环 甲=0; 甲<长度(数组); 甲=甲+1 {
        元素 = 数组[甲];
        如果 !是否在数组中(结果, 元素) {
            结果 = 追加(结果, 元素);
        }
    }
    返回 结果;
}

数组 = [1, 2, 2, 3, 4, 4, 5, 5, 5, 1, 2, 1111, 2222, 111, 1111];
去重_数组 = 去重(数组);
打印("去重数组: " + 去重_数组);
