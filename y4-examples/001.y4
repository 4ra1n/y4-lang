函数 冒泡排序(数组) {
    长 = 长度(数组);
    循环 甲=0; 甲<长-1; 甲=甲+1 {
        循环 乙=1; 乙<长-甲;乙=乙+1 {
            如果 数组[乙-1] > 数组[乙] {
                临时 = 数组[乙-1];
                数组[乙-1] = 数组[乙];
                数组[乙] = 临时;
            }
        }
    }
}

