// Bubble Sort

// function bubble_sort
def bubble_sort(arr) {
    len = length(arr);
    for i=0; i<len-1; i=i+1 {
        for j=1; j<len-i; j=j+1 {
            if arr[j-1] > arr[j] {
                temp = arr[j-1];
                arr[j-1] = arr[j];
                arr[j] = temp;
            }
        }
    }
}

// test code
test_arr = [1, 3, 2, 4, 666, 50];
bubble_sort(test_arr);
print(test_arr);